<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wonhoi_seoul_subway_info_app_for_global">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name="com.example.wonhoi_seoul_subway_info_app_for_global.SubwayApplication"
        android:theme="@style/Theme.wonhoi_seoul_subway_info_app_for_global"
        android:usesCleartextTraffic="true">
        <activity android:name="com.example.wonhoi_seoul_subway_info_app_for_global.presentation.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>